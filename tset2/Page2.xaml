<Page x:Class="tset2.Page2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:tset2"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page2">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button Height="40" Width="100" Click="Button_Click">Выйти</Button>
        <Button Height="40" Width="250" Grid.Row="1" Click="Button_Click_1">Скачать из другой папки</Button>
        <Button Height="40" Width="250" Grid.Row="2" Click="Button_Click_2" Content="Скачать из бд"/>
        <Button Height="40" Width="250" Grid.Row="3" Content="kanban" Click="Button_Click_3"/>
    </Grid>
</Page>
